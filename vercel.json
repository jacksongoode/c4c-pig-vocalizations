{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/upload.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "api/upload.py"
    },
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ]
}
